    "script": "curl -sSL https://raw.githubusercontent.com/Kandil7/prprompts-flutter-generator/master/scripts/setup-gemini-gist.sh | bash",
    "manual": [
      "git clone https://github.com/Kandil7/prprompts-flutter-generator.git",
      "cd prprompts-flutter-generator",
      "./scripts/install-gemini-commands.sh --global"
    ]
  },
  "quickStart": {
